# ------------ User push buttons ----------------------------------------------
set_io --warn-no-port BTN 31  # input

#------ User LEDs
set_io --warn-no-port LEDS[0] 4  #-- output 
set_io --warn-no-port LEDS[1] 3  #-- output
set_io --warn-no-port LEDS[2] 2  #-- output 
set_io --warn-no-port LEDS[3] 1  #-- output

# -------------------------- SYSTEM CLOCK -------------------------------------
set_io --warn-no-port clk 94  # input

# ------------ Reset ---------------
set_io --warn-no-port reset 37  # input